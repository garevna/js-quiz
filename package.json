{
  "name": "js-quiz",
  "version": "2.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "github-build": "vite build --base=/js-quiz/",
    "lint": "vue-cli-service lint",
    "dev": "vite",
    "deploy": "git add dist && git commit -m 'Initial dist subtree commit' && git subtree push --prefix dist origin master:gh-pages"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "roboto-fontface": "*",
    "vue": "3.2.47",
    "vuetify": "3.5.13",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vite": "^3.0.8",
    "vite-plugin-vuetify": "1.0.2",
    "vue-cli-plugin-vuetify": "~2.5.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
